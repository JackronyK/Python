    "def IntFd(lst,target):\n",
    "    for i in range(len(lst)):\n",
    "        for j in range(i+1,len(lst)):\n",
    "            if lst[i] + lst[j] == target:\n",
    "                return f\"lst[{i}] and lst[{j}]\"\n",
    "    return f\"None\"\n",
    "\n"
